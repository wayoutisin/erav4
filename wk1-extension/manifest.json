{
  "manifest_version": 3,
  "name": "TabGenie",
  "version": "1.0",
  "description": "Organize your tabs with a single click based on page title similarity.",
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
